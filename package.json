{
  "name": "universalsnowpack",
  "type": "module",
  "version": "0.1.0",
  "description": "Example universal Javascript app using Snowpack",
  "repository": {
    "type": "git",
    "url": "https://github.com/beejunk/universal-pika-example"
  },
  "main": "server/index.js",
  "scripts": {
    "start": "node --experimental-modules server/index.js",
    "dev": "npx nodemon --inspect --experimental-modules server/index.js",
    "prepare": "snowpack install --clean"
  },
  "keywords": [
    "games",
    "writing"
  ],
  "author": "Brian David <beejunk@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "express": "^4.17.1",
    "htm": "^2.2.1",
    "preact": "^10.2.1",
    "preact-render-to-string": "^5.1.3",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "snowpack": "^1.0.3"
  },
  "snowpack": {
    "webDependencies": [
      "preact",
      "preact/debug",
      "preact/hooks",
      "preact-render-to-string",
      "htm",
      "htm/preact",
      "prop-types"
    ]
  }
}
