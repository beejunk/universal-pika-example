{
  "name": "universalpika",
  "type": "module",
  "version": "0.0.1",
  "description": "Example universal Javascript app using Pika",
  "main": "server/index.js",
  "scripts": {
    "start": "node --experimental-modules server/index.js",
    "dev": "npx nodemon --inspect --experimental-modules server/index.js",
    "prepare": "npx @pika/cli install --clean"
  },
  "keywords": [
    "games",
    "writing"
  ],
  "author": "Brian David <beejunk@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "express": "^4.17.1",
    "htm": "^2.2.1",
    "preact": "^10.0.5",
    "preact-render-to-string": "^5.1.1",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@pika/web": "^0.6.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1"
  },
  "@pika/web": {
    "webDependencies": [
      "preact",
      "preact/debug",
      "preact/hooks",
      "preact-render-to-string",
      "htm",
      "htm/preact",
      "prop-types"
    ]
  }
}
